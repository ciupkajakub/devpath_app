<%= form_with(model: category) do |form| %>

  <div class='field'>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class='field'>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>
  <div class='field'>
    <%= form.submit %>
  </div>
<% end %>
